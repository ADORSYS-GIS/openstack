mechanism_drivers = linuxbridge,l2population
extension_drivers = port_security

[ml2_type_flat]
flat_networks = provider

[ml2_type_vxlan]
vni_ranges = {{ vni_ranges }}

[securitygroup]
enable_ipset = {{ enable_ipset | bool | lower }}